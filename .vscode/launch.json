    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Movie VS API",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {},
            "args": [],
            "showLog": true,
            "buildFlags": "-tags=dev"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": "${command:pickProcess}"
        }
    ]
}